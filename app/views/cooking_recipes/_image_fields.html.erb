<% if f.index > 0 && (f.index % 6) == 0 %>
  <div class="clearfix"></div>
<% end %>
<div class="col-md-2">
  <div class="thumbnail">
    <%= f.hidden_field :id %>
    <%= image_tag f.object.file_url(:thumb) %>
    <div class="caption">
      <%= f.hidden_field :_destroy %>
      <%= link_to '#', class:'btn btn-default btn-block destroy_fields' do %>
        <%= fa_icon('trash-o') %> Remove
      <% end %>
    </div>
  </div>
</div>